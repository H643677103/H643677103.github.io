.remote_font #temperatureNum,
.remote_font .limit {
    font-size: 30px;
    color: #356067;
}

[data-theme="dark"] .remote_font #temperatureNum,
[data-theme="dark"] .remote_font .limit {
    color: #fff;
}

#speed_box {
    position: absolute;
    /* display: flex; */
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 90%;
    height: 90%;
    /* border: 1px solid #000; */
    border-radius: 50%;
    background: #FFF;
    font-size: 20px;
    color: #356067;
    align-items: center;
    justify-content: center;
    display: none;
    transition: display .5s;
}

[data-theme="dark"] #speed_box {
    background: rgba(225,225,225,0.2);
    backdrop-filter: blur(20px);
    color: #fff;
}

.remote .switch_center #remote_pattern {
    position: absolute;
    margin-top: 60px;
    left: 50%;
    transform: translateX(-50%);
    color: #356067;
}

[data-theme="dark"] .remote .switch_center #remote_pattern {
    color: #fff;
}

.mod_box {
    width: 100%;
    height: auto;
    margin-top: 40px;
    position: relative;
}

#mod_button {
    position: absolute;
    top: 50%;
    left: 30px;
    transform: translate(-50%, -50%);
    overflow: hidden;
}

#fan-speed-button {
    position: absolute;
    top: 50%;
    right: 30px;
    transform: translate(50%, -50%);
    overflow: hidden;

}

