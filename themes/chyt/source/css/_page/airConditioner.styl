.airLabel{
  position: relative;
  display: block;
  width: 90%;
  top: 50px;
  margin: 0 auto;
  padding-left: 30px;
  border-left: 4px solid #425aef;
  background-color: var(--blockquote-bg);
  border-top-right-radius: 6px;
  border-bottom-right-radius: 6px;
  color: #6a737d;
}

.airLabel>span {
    display:block;
    padding: 10px 0;
    padding-right: 10px;
    font-size: 16px;
}

@font-face {
    font-family: 'led regular';
    src: url('/img/airConditioner/DS-DIGI.TTF');
}

.air-conditioner {
    height: 800px;
}

.air-conditioner-main-top-tit {
    position: relative;
    top: 100px;
    text-align: center;
    font-size: 32px
    font-weight: 700;
}

.air-conditioner-tip {
    position: relative;
    display: flex
    top: 120px;
    width: 220px;
    margin: 0 auto;
    text-align: center;
}

