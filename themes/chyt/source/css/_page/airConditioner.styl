.air-conditioner-tip .air-conditioner-tip-content {
    text-align: center;
}

.air-conditioner-tip .air-conditioner-tip-highlight {
    color: #63a5ef;
}

.air-container {
    position: relative;
    display: block;
    top: 150px;
    margin-bottom: 50px;
    justify-content: center;
    /*  border: 1px solid rgb(201, 201, 201); */
}

.air {
    position: relative;
    margin: 0 auto;
    width: 450px;
    height: 120px;
    background-color: rgb(255, 255, 255);
    border-radius: 10px 10px 0 0;
    filter: drop-shadow(0 2px 10px rgba(0, 0, 0, 0.3));
}

[data-theme="dark"] .air {
    background-color: transparent;
    border: 1px solid #fff;
}

.air::after {
    content: '';
    position: absolute;
    bottom: -21px;
    left: 0;
    height: 20px;
    width: 100%;
    border-top: 1px solid #aaa;
    background-color: rgb(255, 255, 255);
    border-radius: 0 0 20px 20px;
}

[data-theme="dark"] .air::after {
    background-color: transparent;
    border: 1px solid #fff;
}

.feng {
    position: absolute;
    left: 50%;
    /* margin-top: -10px; */
    transform: translateX(-50%);
    border-radius: 0 0 100% 100%;
    width: 410px;
    height: 30px;
    filter: blur(20px);
    background: none;
    transition: all 1s;
    z-index: -1;
}

[data-theme="dark"] .feng{
    display: none;
}

@media screen and (max-width: 768px) {
    .air {
        width: 90%;
    }

    .feng{
        width: 80%;
    }
}

.picture {
    position: absolute;
    top: 10px;
    left: 10px;
    width: 40px;
    height: 65px;
    border-radius: 5px;
}

.pattern {
    position: absolute;
    top: 25px;
    right: 58px;
    width: 20px;
    height: 20px;
    opacity: 0;
    transition: all 0.3s;
}

.air_speed {
    position: absolute;
    top: 27px;
    right: 28px;
    width: 16px;
    height: 16px;
    opacity: 0;
    transition: all 0.3s;
}

.font {
    /* 引用LED字体 */
    font-family: "led regular";
    position: absolute;
    right: 32px;
    top: 48px;
    width: 70px;
    height: 30px;
    line-height: 30px;
    font-size: 32px;
    font-weight: 400;
    color: rgb(184, 184, 184);
    transition: all 0.3s;
}

.air-logo {
    position: absolute;
    left: 50%;
    bottom: 3px;
    transform: translateX(-50%);
    font-size: 15px;
    color: rgb(139, 139, 139);
}

.dot {
    position: absolute;
    bottom: -8px;
    right: 12px;
    width: 7px;
    height: 5px;
    border-radius: 50%;
    z-index: 1;
    background-color: rgb(112, 112, 112, 0.5);
    transition: all 0.3s;
}

.wind {
    position: relative;
    top: 30px;
    width: 250px;
    height: 60px;
    margin: 0 auto;
    opacity: 0;
    transition: all 1.2s;
    animation: move 1.5s ease-in-out infinite;
}

@keyframes move {

    0%,
    100% {
        transform: translateY(0px);
    }

    50% {
        transform: translateY(5px);
    }
}

.wind .winds {
    position: absolute;
    width: 5px;
    height: 40px;
    background: rgb(206, 206, 206);
    transition: opacity .5s;
}

/* .speed_low,
.speed_mid,
.speed_hig{
    opacity: 0;
} */

.wind1 {
    left: 20%;
    transform: rotate(20deg);
}

.wind2 {
    left: 50%;
}

.wind3 {
    left: 80%;
    transform: rotate(-20deg);
}

.wind4 {
    left: -10%;
    transform: rotate(30deg);
}

.wind5 {
    left: 110%;
    transform: rotate(-30deg);
}

.wind6 {
    left: -40%;
    transform: rotate(40deg);
}

.wind7 {
    left: 140%;
    transform: rotate(-40deg);
}

.remote {
    position: relative;
    top: 70px;
    margin: 0 auto;
    padding: 10px;
    width: 200px;
    height: 300px;
    box-shadow: 0 0 10px 5px #aaaaaa30;
    border-radius: 20px;
    overflow: hidden;
}

[data-theme="dark"] .remote {
    border: 1px solid #fff;
}

.remote .open_box {
    position: relative;
    width: 100%;
    height: 40px;
}

.remote .open_box #power-button {
    position: absolute;
    top: 50%;
    left: 20px;
    transform: translate(-50%, -50%);
}

