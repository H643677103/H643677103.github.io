.remote .open_box span {
    position: absolute;
    display: block;
    right: 10px;
    line-height: 40px;
    font-size: 16px;
}

.remote .switch_center {
    position: relative;
    display: flex;
    align-items: center;
    width: 160px;
    height: 160px;
    margin: 0 auto;
    margin-top: 10px;
    /* border: 1px solid #000; */
}

.remote .switch_center .circle-ring {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
    height: 100%;
    /* border: 15px solid #F4F8F9; */
    border-radius: 50%;
    box-shadow: 0 0 20px 10px #e5f4f8, inset 0 0 20px 10px #e5f4f8;
    background-color: transparent;
    z-index: -2;
}

.remote .switch_center #circle-ring_2 {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 0;
    height: 0;
    border-radius: 50%;
    box-shadow: #A9EEF5;
    z-index: -1;
    transition: all .5s;
}

.remote .switch_center .remote_font {
    display: flex;
    justify-content: space-between;
    width: 90%;
    margin: 0 auto;
}

.remote_font #temp-up-button,
.remote_font #temp-down-button {
    display: flex;
    align-items: center;
    justify-content: center;
}

