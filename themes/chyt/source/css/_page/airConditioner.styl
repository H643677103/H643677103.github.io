.openIs::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: white;
    opacity: 0.5;
    transition: opacity 0.3s;
    /* 添加透明度过渡效果 */
}

[data-theme="dark"] .openIs::before {
    display: none;
}

.remote .open_box #power-button:active,
.remote .mod_box #mod_button:active,
.remote .mod_box #fan-speed-button:active {
    width: 35px;
    height: 35px;
    box-shadow: 0 0 4px 4px #E7ECEF,
        inset 2px 2px 2px #feffff,
        inset -4px -4px 4px #feffff;
}

[data-theme="dark"] .remote .open_box #power-button:active,
[data-theme="dark"] .remote .mod_box #mod_button:active,
[data-theme="dark"] .remote .mod_box #fan-speed-button:active {
    box-shadow: none,
}

#mod_button,
#fan-speed-button,
#power-button {
    width: 40px;
    height: 40px;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: #F4F8F9;
    box-shadow: 0 0 2px 2px #E7ECEF,
        inset 2px 2px 2px #feffff,
        inset -4px -4px 4px #feffff;
    /* border: 1px solid #000; */
}

[data-theme="dark"] .mod,
[data-theme="dark"] .wind_speed,
[data-theme="dark"] .open {
    background-color: transparent;
    box-shadow: none;
    border: 1px solid #fff;
}